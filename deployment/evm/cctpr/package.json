{
  "name": "@stable-io/cctpr-evm-deployment",
  "version": "1.0.0",
  "private": true,
  "description": "CCTPR Deployment scripts",
  "type": "module",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "clean": "make -C ../../../contracts/cctpr/evm/ clean && rm -rf dist build-contracts",
    "build-contracts": "make -C ../../../contracts/cctpr/evm/ && mkdir -p build-contracts && cp -r ../../../contracts/cctpr/evm/out/* ./build-contracts",
    "build": "tsc --build",
    "deploy-cctpr": "yarn tsx deployCctpr.ts",
    "deploy-avax-router": "yarn tsx deployAvaxRouter.ts",
    "verify-etherscan": "./verifyEtherscan.sh",
    "verify-bytecode": "yarn tsx verifyBytecode.ts",
    "set-fees": "yarn tsx setFees.ts"
  },
  "license": "MPL-2.0",
  "devDependencies": {
    "chalk": "^5.4.1",
    "eslint": "^9.22.0",
    "eslint-config": "workspace:^",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "viem": "^2.23.12"
  },
  "dependencies": {
    "@stable-io/cctp-sdk-cctpr-evm": "workspace:^",
    "@stable-io/cctp-sdk-definitions": "workspace:^",
    "@stable-io/utils": "workspace:^"
  }
}
