{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@stable-io/utils",
  "version": "0.0.4",
  "type": "module",
  "license": "MPL-2.0",
  "author": "Stable Technologies",
  "description": "Utility functions for Stable packages",
  "keywords": [
    "typescript",
    "cctp",
    "sdk"
  ],
  "exports": {
    "types": "./dist/index.d.ts",
    "require": "./dist/index.cjs",
    "import": "./dist/index.js",
    "default": "./dist/index.js"
  },
  "files": [
    "dist",
    "!*.test.*",
    "README.md"
  ],
  "scripts": {
    "prepublishOnly": "eslint . && jest && tsc",
    "build": "tsc",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "jest --coverage",
    "test:watch": "yarn test --watchAll=true",
    "clean": "rm -rf ./dist"
  },
  "dependencies": {
    "@noble/curves": "^1.4.0",
    "@noble/hashes": "^1.3.1",
    "@scure/base": "^1.2.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "eslint": "^9.22.0",
    "eslint-config": "workspace:^",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.6",
    "typescript": "^5.8.2"
  }
}
