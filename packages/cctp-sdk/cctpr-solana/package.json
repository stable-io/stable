{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@stable-io/cctp-sdk-cctpr-solana",
  "version": "0.0.6",
  "type": "module",
  "license": "MPL-2.0",
  "author": "Stable Technologies",
  "description": "Solana support for the CCTPR corridor of the CCTP SDK",
  "keywords": [
    "typescript",
    "cctp",
    "sdk"
  ],
  "exports": {
    "types": "./dist/index.d.ts",
    "require": "./dist/index.cjs",
    "import": "./dist/index.js",
    "default": "./dist/index.js"
  },
  "files": [
    "dist/**/*",
    "!*.test.*",
    "README.md"
  ],
  "scripts": {
    "prepublishOnly": "eslint . && tsc",
    "build": "tsc",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "clean": "rm -rf ./dist"
  },
  "dependencies": {
    "@stable-io/cctp-sdk-cctpr-definitions": "workspace:^",
    "@stable-io/cctp-sdk-definitions": "workspace:^",
    "@stable-io/cctp-sdk-solana": "workspace:^",
    "@stable-io/map-utils": "workspace:^",
    "@stable-io/utils": "workspace:^",
    "binary-layout": "^1.3.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@codama/nodes-from-anchor": "^1.2.1",
    "@codama/renderers-js": "^1.2.14",
    "codama": "^1.3.0",
    "eslint": "^9.22.0",
    "eslint-config": "workspace:^",
    "typescript": "^5.8.2"
  }
}
